"@AcmeApiClientBundle/Resources/public/js/faqController.js"
"@AcmeApiClientBundle/Resources/public/js/statController.js"
"@AcmeApiClientBundle/Resources/public/js/resetController.js"
"@AcmeApiClientBundle/Resources/public/js/md-data-table.js"
